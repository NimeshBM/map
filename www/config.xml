<?xml version='1.0' encoding='utf-8'?>
<widget id="com.www.map" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>My Map</name>
    <description>
        Geolocation Example
    </description>
    <author email="nmithil99@gmail.com">
        Mithil BHOOTOOA
    </author>
    <content src="index.html" />

 <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
  </platform>
    <plugin name="cordova-plugin-geolocation"         source="npm" spec="~2.4.3" />
    <plugin name="cordova-plugin-globalization"       source="npm" spec="~1.0.7" />
    <plugin name="cordova-plugin-inappbrowser"        source="npm" spec="~1.7.1" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~1.3.3" />
    <plugin name="cordova-plugin-vibration"           source="npm" spec="~2.1.5" />
    <plugin spec="https://github.com/litehelpers/Cordova-sqlite-evcore-extbuild-free.git#0.8.2" source="git" />


</widget>
